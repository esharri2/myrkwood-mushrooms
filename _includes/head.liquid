<head>
    {% render 'meta',
        page_title: title,
        site_title: site.title,
        page_description: description,
        site_description: site.description,
        author: site.author,
        domain: site.domain,
        meta_image: site.meta_image,
        meta_image_alt: site.meta_image_alt
    %}
    <link
        as="font"
        crossorigin
        href="fraunces-v3-latin-regular.woff2"
        rel="preload"
        type="font/woff2" />

    <link
        href="/main.css"
        rel="stylesheet"
        type="text/css" />
    <script
        src="/main.js"
        type="module"></script>
</head>
